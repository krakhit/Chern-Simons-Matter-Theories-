Cell[TextData[{
 "rules for computing S matrices : T channel - particle anti particle)\n\n\
Incoming states : (momentum assignments to be considered in off-shell four \
point function\np+q (particle), -k-q (anti particle) all energies are \
positive convention\n\nOutgoing states: p(particle), -k anti particle\n\n\
Algorithm for wavefunction substitution in the correlation function: ",
 Cell[BoxData[{
  RowBox[{
   RowBox[{
    RowBox[{
    "1.", " ", "Identify", " ", "process", " ", "in", " ", "momentum", " ", 
     "space"}], ",", " ", 
    RowBox[{
     RowBox[{"the", " ", "correlator", " ", "V"}], "=", " ", 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[Psi]bar", "+"], 
       RowBox[{"(", 
        RowBox[{"p", "+", "q"}], ")"}], " ", 
       SuperscriptBox["\[Psi]", "+"], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "k"}], "-", "q"}], ")"}]}], "\[Rule]", " ", 
      RowBox[{
       SuperscriptBox["\[Psi]bar", "+"], 
       RowBox[{"(", 
        RowBox[{"-", "k"}], ")"}], " ", 
       SuperscriptBox["\[Psi]", "+"], 
       RowBox[{"(", "p", ")"}]}]}]}]}], " "}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
   "2.", " ", "What", " ", "we", " ", "compute", " ", "is", " ", "the", " ", 
    "corresponding", " ", "term", " ", "in", " ", "the", " ", "effective", 
    " ", "action"}], ",", " ", 
   RowBox[{
    RowBox[{"which", " ", "is", " ", 
     SuperscriptBox["\[Psi]", "+"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", "p"}], "-", "q"}], ")"}], " ", 
     SuperscriptBox["\[Psi]bar", "+"], 
     RowBox[{"(", 
      RowBox[{"k", "+", "q"}], ")"}]}], "\[Rule]", " ", 
    RowBox[{
     SuperscriptBox["\[Psi]", "+"], 
     RowBox[{"(", "k", ")"}], " ", 
     SuperscriptBox["\[Psi]bar", "+"], 
     RowBox[{"(", 
      RowBox[{"-", "p"}], ")"}], " ", "V"}]}], ",", " ", 
   RowBox[{
   "and", " ", "then", " ", "we", " ", "take", " ", "the", " ", "onshell", 
    " ", 
    RowBox[{"limit", ".", " ", "This"}], " ", "process", " ", "will", " ", 
    "give", " ", "the", " ", "S", " ", "matrix", " ", "corresponding", " ", 
    "to", " ", "the", " ", "process", " ", "of", " ", 
    RowBox[{"interest", ".", "\[IndentingNewLine]", "3."}], " ", "Rules", " ",
     "for", " ", "wave", " ", 
    RowBox[{"function", ":", " ", 
     RowBox[{
     "the", " ", "term", " ", "in", " ", "the", " ", "effective", " ", 
      "action", " ", "is", " ", "the", " ", "conjugate", " ", "of", " ", 
      "the", " ", "term", " ", "represented", " ", "in", " ", "the", " ", 
      "correlator", " ", 
      RowBox[{"(", 
       RowBox[{"time", " ", "direction", " ", "is", " ", "reversed"}], 
       ")"}]}]}]}], ",", " ", 
   RowBox[{
   "the", " ", "onshell", " ", "wave", " ", "function", " ", "substitution", 
    " ", "rule", " ", "follows", " ", "from", " ", "the", " ", "mode", " ", 
    "expansions"}]}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"\[Psi]", "~", " ", "u"}], " ", "\[Alpha]"}], " ", "+", " ", 
   RowBox[{"v", " ", 
    SuperscriptBox["\[Alpha]c", "dagger"]}]}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"\[Psi]bar", "~", " ", "ubar"}], " ", 
    SuperscriptBox["\[Alpha]", "dagger"]}], " ", "+", " ", 
   RowBox[{"vbar", " ", "\[Alpha]c"}]}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    SuperscriptBox["a", "dagger"], " ", "creates", " ", "particles"}], ",", 
   " ", 
   RowBox[{
    RowBox[{
     SuperscriptBox["\[Alpha]c", "dagger"], " ", "creates", " ", "anti", " ", 
     
     RowBox[{"particles", ".", "\[IndentingNewLine]", "4."}], " ", 
     "Substitution", " ", 
     RowBox[{"rule", ":", "\[IndentingNewLine]", 
      RowBox[{
       SuperscriptBox["\[Psi]", "+"], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "p"}], "-", "q"}], ")"}]}]}]}], "\[Rule]", " ", 
    RowBox[{"u", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", "p"}], "-", "q"}], ")"}], " ", "\[Alpha]", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", "p"}], " ", "is", " ", "just", " ", "reversal", " ", "of",
        " ", "spatial", " ", "momentum"}], ")"}]}]}]}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    SuperscriptBox["\[Psi]bar", "+"], 
    RowBox[{"(", 
     RowBox[{"k", "+", "q"}], ")"}]}], "\[Rule]", " ", 
   RowBox[{"vbar", 
    RowBox[{"(", 
     RowBox[{"k", "+", "q"}], ")"}], " ", 
    "\[Alpha]c"}]}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     SuperscriptBox["\[Psi]", "+"], 
     RowBox[{"(", "k", ")"}]}], "\[Rule]", " ", 
    RowBox[{"v", 
     RowBox[{"(", "k", ")"}], " ", 
     SuperscriptBox["\[Alpha]c", "dagger"]}]}], " "}], "\[IndentingNewLine]", 
 
  RowBox[{" ", 
   RowBox[{
    RowBox[{
     SuperscriptBox["\[Psi]bar", "+"], 
     RowBox[{"(", 
      RowBox[{"-", "p"}], ")"}]}], "\[Rule]", " ", 
    RowBox[{"ubar", 
     RowBox[{"(", 
      RowBox[{"-", "p"}], ")"}], " ", 
     SuperscriptBox["\[Alpha]", "dagger"]}]}]}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
   "4.", " ", "Once", " ", "all", " ", "the", " ", "wavefunction", " ", 
    "substituions", " ", "are", " ", "done"}], ",", " ", 
   RowBox[{
   "Simplify", " ", "and", " ", "check", " ", "for", " ", "singularities"}], 
   ",", " ", "\[IndentingNewLine]", 
   RowBox[{"Carefully", " ", "take", " ", "the", " ", "onshell", " ", 
    RowBox[{"limit", "."}]}]}]}],
  CellChangeTimes->{{3.692874073388048*^9, 3.692874315101726*^9}, {
   3.692874384940444*^9, 3.6928747923791027`*^9}, {3.6928748574590282`*^9, 
